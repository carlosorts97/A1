    <?php
      $num='';
      $num=htmlspecialchars($_POST['num']);
      if($num==0){
        echo'No se puede dividir el 0';
      }

    for ($i=1; $i <= $num ; $i++) {
        if($num%$i==0){
          echo '<br>'.$i;
        }
    }
     ?>

    <form action="<? $_SERVER['PHP_SELF']; ?>" method="post">
      <p>NÃºmero:<input type="text" name="num"></p>
